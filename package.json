{
  "name": "genesisuniverse-core",
  "version": "0.0.1",
  "scripts": {
    "prepare": "node ./.setup.js",
    "test": "npx hardhat test",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "build": "npm run clean && npm run compile",
    "lint": "eslint \"**/*.{js,ts}\" && solhint contracts/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix contracts/**/*.sol",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "coverage": "npx hardhat coverage",
    "sizer": "hardhat size-contracts",
    "gas": "cross-env REPORT_GAS=true npx hardhat test",
    "execute": "node ./scripts.js run",
    "deploy": "node ./scripts.js deploy",
    "verify": "node ./scripts.js verify",
    "export": "node ./scripts.js export",
    "fork:execute": "node ./scripts.js fork:run",
    "fork:deploy": "node ./scripts.js fork:deploy",
    "fork:dev": "node ./scripts.js fork:dev",
    "fork:test": "node ./scripts.js fork:test",
    "fork:node": "node ./scripts.js fork:node"
  },
  "engines": {
    "node": ">=14.14.0"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.4.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.13",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",

    "@openzeppelin/contracts": "^3.4.2",
    "@openzeppelin/contracts-upgradeable": "^3.4.2",
    "@openzeppelin/hardhat-upgrades": "^1.16.1",
    "@openzeppelin/upgrades-core": "^1.15.0",

    "@openzeppelin/contracts-v0.8": "npm:@openzeppelin/contracts@^4.6.0",
    "@openzeppelin/contracts-upgradeable-v0.8": "npm:@openzeppelin/contracts-upgradeable@^4.6.0",

    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-havven": "^1.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "ethers": "^5.6.4",
    "hardhat": "^2.9.9",
    "hardhat-abi-exporter": "^2.0.8n",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-deploy": "^0.11.10",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-preprocessor": "^0.1.4",
    "hardhat-spdx-license-identifier": "^2.0.0",
    "hardhat-watcher": "^2.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "tsort": "^0.0.1",
    "commander": "^9.2.0"
  }
}
